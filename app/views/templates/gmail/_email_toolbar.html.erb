<div class="email-list-toolbar" xmlns="http://www.w3.org/1999/html" xmlns="http://www.w3.org/1999/html">
  <div class="btn-toolbar float-left">
    <span class="checkbox-button"></span>
    <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
    <label class="form-check-label" for="defaultCheck1">
    </label>

    <button type="button" class="btn btn-light" id="email-caret-down-button"><i class="fa fa-caret-down" aria-hidden="true"></i></button>
    <button type="button" class="btn btn-light" id="repeat-button"><i class="fa fa-repeat" aria-hidden="true"></i></button>
    <button type="button" class="btn btn-light" id="email-toolbar-buttons"><i class="fa fa-ellipsis-v" aria-hidden="true"></i></button>
  </div>
  <div class="btn-toolbar float-right">
    <button type="button" class="btn btn-light" id="email-toolbar-buttons"><i class="fa fa-chevron-left" aria-hidden="true"></i></button>
    <button type="button" class="btn btn-light" id="email-toolbar-buttons"><i class="fa fa-chevron-right" aria-hidden="true"></i></button>
    <button type="button" class="btn btn-light" id="email-toolbar-buttons"><i class="fa fa-keyboard-o" aria-hidden="true"></i></button>
    <button type="button" class="btn btn-light" id="arrow-circle-down"><i class="fa fa-arrow-circle-down" aria-hidden="true"></i></button>
    <button type="button" class="btn btn-light" id="cog-button"><i class="fa fa-cog" aria-hidden="true"></i></button>
  </div>
</div>
