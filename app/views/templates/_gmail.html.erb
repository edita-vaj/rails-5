<div id="gmail" class="container-fluid">
  <header class="row">
    <div class="col-2">
      <i class="fa fa-bars" aria-hidden="true"></i>
      <img src="https://static.digit.in/default/4bf2e863380c26afce61fa97c2c71401eadd5656.png" class="mail-image">
      <p class="top-left-gmail-icon"><strong>Gmail</strong></p>
    </div>
    <div class="col-8">
      <%= render('templates/gmail/search') %>
    </div>
    <div class="col-2">
      <div class="list-group-item">
      <i class="fa fa-question-circle-o" aria-hidden="true"></i>
      <img src="https://cdn3.iconfinder.com/data/icons/essentials-set-2/17/007-menu-512.png" class="dotted-menu">
      <img src="https://macstickers.com/wp-content/uploads/2018/07/Letter-E.png" class="e-letter-menu">
    </div>
    </div>

  </header>
  <div class="row mt-2">
    <div class="col-2">
      <div class="row">
        <button class="new-email">
          <img src="https://www.gstatic.com/images/icons/material/colored_icons/1x/create_32dp.png"> Compose
        </button>
      </div>
      <div class="row nav-folders">
        <div class="col-12"><%= render('templates/gmail/side_folders') %></div>
      </div>
    </div>
    <div class="col-10">
      <div class="middle-column email-list">
      <i class="fa fa-square-o" aria-hidden="true"></i>
      </div>
    </div>
  </div>
</div>







